{
    "name": "open-orchestra/open-orchestra-media-bundle",
    "description": "A media bundle for the OpenOrchestra Project",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "OpenOrchestra community",
            "homepage": "https://github.com/open-orchestra/open-orchestra-media-bundle/graphs/contributors"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "ext-imagick": "*",
        "open-orchestra/open-orchestra-model-interface": "*",
        "open-orchestra/open-orchestra-display-bundle": "*",
        "open-orchestra/open-orchestra-bbcode-bundle": "*",
        "open-orchestra/open-orchestra-mongo-libs": "*",
        "open-orchestra/open-orchestra-media-file-bundle": "*",
        "doctrine/data-fixtures":"1.1.1",
        "doctrine/mongodb-odm": "1.0.2",
        "doctrine/mongodb-odm-bundle" : "3.0.1",
        "stof/doctrine-extensions-bundle": "1.2.1",
        "php-ffmpeg/php-ffmpeg": "~0.6"
    },
    "require-dev": {
        "open-orchestra/open-orchestra-base-bundle": "dev-master",
        "psr/log": "1.0.0",
        "codeclimate/php-test-reporter": "dev-master",
        "symfony/validator": "2.7.0",
        "jmikola/geojson": "1.0.1",
        "phpunit/phpunit" : "4.7.2",
        "phake/phake": "2.1.0"
    },
    "config" : {
        "bin-dir" : "bin"
    },
    "autoload": {
        "psr-4": {
            "OpenOrchestra\\MediaModelBundle\\": "MediaModelBundle/",
            "OpenOrchestra\\MediaBundle\\": "MediaBundle/",
            "OpenOrchestra\\Media\\": "Media/"
        }
    }
}
