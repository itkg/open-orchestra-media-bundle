<div id="{{galleryId}}" class="gallery {{ galleryClass }}">
    <div>
    {% for picture in pictures %}<a 
        class="fancybox-thumb"
        rel="fancybox-thumb"
        href="{{ get_media_format_url(picture, imageFormat) }}"
        title="{{ get_media_title(picture) }}"
       ><img src="{{ get_media_format_url(picture, thumbnailFormat) }}" alt="{{ get_media_alt(picture) }}" class="gallery-picture" /></a>{% endfor %}
    </div>
</div>
{% if (numberOfPages > 1) %}
    <div class="gallery-navigator">
        {{ navigator(numberOfPages, currentPage, parameters) }}
    </div>
{%endif%}

<script type="text/javascript">
    if (typeof orchestraGalCol == 'undefined') {
        var orchestraGalCol = {};
    }
    orchestraGalCol['{{galleryId}}'] = {{ numberOfColumns }};
    require(['{{ asset('/bundles/openorchestramedia/block/Gallery/blockLoader.js') }}']);
</script>
